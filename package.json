{
  "name": "generator-icelandair",
  "version": "0.2.1",
  "description": "Scaffold out a new project",
  "license": "UNLICENSED",
  "repository": "Icelandair/generator-icelandair",
  "author": {
    "name": "Benedikt D Valdez",
    "email": "benediktvaldez@gmail.com",
    "url": "http://bdvs.info"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel src -d generators --ignore templates && ./bin/copy",
    "dev": "supervisor --extensions node,js,json,go,yml,md --watch src --no-restart-on exit --exec npm -- run build",
    "comment": "babel -- --ignore templates --out-dir generators src",
    "lint": "eslint src/*/index.js --fix",
    "test": "ava"
  },
  "files": [
    "generators",
    "util"
  ],
  "keywords": [
    "yeoman-generator",
    "plugin",
    "boilerplate",
    "template",
    "scaffold",
    "node",
    "module",
    "node_module",
    "node-module",
    "init"
  ],
  "dependencies": {
    "babel-register": "^6.7.2",
    "chalk": "^1.1.3",
    "humanize-url": "^1.0.1",
    "normalize-url": "^1.2.0",
    "rimraf": "^2.5.2",
    "superb": "^1.1.3",
    "underscore.string": "^3.0.3",
    "yeoman-generator": "^0.22.1"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "eslint": "^2.9.0",
    "eslint-config-icelandair": "^1.0.0",
    "eslint-plugin-import": "^1.7.0",
    "ncp": "^2.0.0",
    "pify": "^2.2.0",
    "supervisor": "^0.10.0",
    "yeoman-assert": "^2.0.0",
    "yeoman-test": "^1.0.0"
  }
}
